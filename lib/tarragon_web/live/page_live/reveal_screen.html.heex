<!-- Hero Information (Class, Abilities) -->
<div class="HeroInfoBlock w-full h-[216px] relative flex flex-col items-center gap-[20px]">
    <!-- Hero Name -->
    <div class="Luna text-[#dbd7d5] text-[24px] font-bold font-metropolis-bold mt-[10px]">Luna</div>

    <!-- Hero Class Icons -->
    <div class="HeroClassIcons w-[300px] h-[42px] flex justify-center gap-[20px] items-center mt-[25px]">
        <!-- Separator Left -->
        <div>
            <svg width="69" height="4" viewBox="0 0 69 4" fill="none" xmlns="http://www.w3.org/2000/svg">
                <path d="M0 2C73.6 2 73.6667 2 64.5 2" stroke="#AD9E88" stroke-width="3"/>
            </svg>
        </div>

        <!-- First Icon -->
        <div class="ClassIconGroup relative w-[42px] h-[42px] flex items-center justify-center mr-[10px]">
            <div class="ClassIconBackground w-[42px] h-[42px] bg-[#51677b] rounded-full border border-[#ad9e88]"></div>
            <img class="ClassIconImage w-[27px] h-[27px] absolute" src="/images/ghille-suit-class.png" />
        </div>

        <!-- Second Icon -->
        <img class="ClassIcon w-[42px] h-[42px]" src="/images/scope-class.png" />

        <!-- Separator Right -->

        <div>
            <svg width="69" height="4" viewBox="0 0 69 4" fill="none" xmlns="http://www.w3.org/2000/svg">
                <path d="M0 2C73.6 2 73.6667 2 64.5 2" stroke="#AD9E88" stroke-width="3"/>
            </svg>
        </div>
    </div>

    <!-- Skill Icons -->
    <div class="SkillIcons w-[159px] h-[70px] flex justify-center gap-[20px] mt-[35px]">
        <!-- Action Badge 1 -->
        <div class={["relative w-[70px] h-[70px]"]}>
            <div class="ActionBackground w-[70px] h-[70px] bg-[#28343e] rounded-[10px] border border-[#ad9e88]"></div>
            <img class="ActionIcon w-[60px] h-[60px] absolute top-[5px] left-[5px] rounded-[7px] border border-[#3c4e5d]" src="/images/ghille-suit-skill.png" />
        </div>

        <!-- Action Badge 2 -->
        <div class={["relative w-[70px] h-[70px]"]}>
            <div class="ActionBackground w-[70px] h-[70px] bg-[#28343e] rounded-[10px] border border-[#ad9e88]"></div>
            <img class="ActionIcon w-[60px] h-[60px] absolute top-[5px] left-[5px] rounded-[7px] border border-[#3c4e5d]" src="/images/critical-damage-skill.png" />
        </div>
    </div>

    <!--Continue-->
    <div class="ContinueText relative top-[50px] mx-auto font-metropolis-bold text-[15px] text-white">Tap anywhere to continue</div>
</div>



