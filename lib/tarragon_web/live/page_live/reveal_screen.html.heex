<div class="absolute halftone-reveal top-0 left-0 w-[100vw] h-[100dvh]">

    <div class="RevealBackground w-full h-full bg-gradient-to-b from-black to-[#C71B1B]" >&nbsp;</div>
</div>


<style>
    @keyframes scaleUp {
        0% {
            transform: scale(0.8);
        }
        70% {
            transform: scale(1.05); /* Overshooting to 105% */
        }
        100% {
            transform: scale(1); /* Final size at 100% */
        }
    }

    .CharacterModel {
        transform: scale(0.8);
        animation: scaleUp 0.3s ease-out forwards;
    }

    .Luna {
        transform: scale(0.8);
        animation: scaleUp 0.3s ease-out forwards;
    }
</style>

<!-- Revealed Hero Model or Avatar -->
<div class="CharacterModel w-[173px] h-[198px] mx-auto mt-[170px] relative">
    <div class="CharacterModelBackground w-[173px] h-[198px] left-0 top-0 absolute bg-[#28343e] rounded-[10px] border border-[#ad9e88]"></div>
    <div class="p-[5px] w-[173px] h-[198px] relative">
        <img class="CharacterModeImage  w-full h-full rounded-[7px] object-cover" src="/images/female-character-reveal.png" />
    </div>
</div>

